<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.N.views.LoginController">
   <children>
      <Label layoutX="133.0" layoutY="5.0" text="Login">
         <font>
            <Font name="Calibri" size="16.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="39.0" text="Username:" />
      <TextField fx:id="txtf_username" layoutX="79.0" layoutY="35.0" />
      <Label layoutX="16.0" layoutY="76.0" text="Password:" />
      <PasswordField fx:id="pwf_password" layoutX="79.0" layoutY="72.0" />
      <Button fx:id="btn_loginSubmit" layoutX="234.0" layoutY="111.0" mnemonicParsing="false" onMouseClicked="#login" text="Submit" />
      <Button fx:id="btn_cancel" layoutX="176.0" layoutY="111.0" mnemonicParsing="false" onMouseClicked="#login" text="Cancel" />
   </children>
</AnchorPane>
