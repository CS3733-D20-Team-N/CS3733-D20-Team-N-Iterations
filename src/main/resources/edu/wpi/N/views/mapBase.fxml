                    <image>
                        <Image url="@../images/Floor4SolidBackground.png" />
                    </image>
                </ImageView>
                <Pane fx:id="pn_path">
                    <children>
                        <Pane fx:id="pn_routeNodes" />
                    </children></Pane>
            </children>
            <cursor>
                <Cursor fx:constant="OPEN_HAND" />
