<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.N.views.HomeController">
   <children>
      <VBox alignment="CENTER" layoutX="640.0" layoutY="519.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fitHeight="233.0" fitWidth="298.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/Brigham_and_Womens_Hospital_logo.svg.png" />
               </image>
            </ImageView>
            <Label alignment="CENTER" contentDisplay="CENTER" minHeight="-Infinity" text="Welcome to Brigham and Womens Hospital" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Calibri" size="36.0" />
               </font>
            </Label>
            <Separator maxWidth="500.0" prefWidth="200.0" />
            <Label contentDisplay="CENTER" text="Click to Continue" textAlignment="CENTER">
               <font>
                  <Font name="Calibri" size="18.0" />
               </font>
            </Label>
            <Button fx:id="btn_map" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#onBtnMapClicked" text="Map" />
