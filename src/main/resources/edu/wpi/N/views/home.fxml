            <Button fx:id="btn_generateTest" contentDisplay="CENTER" layoutX="938.0" layoutY="612.0" mnemonicParsing="false" onMouseClicked="#onBtnGenerateClicked" text="QR - Generate" />
             <Button fx:id="btn_filemanager" contentDisplay="CENTER" layoutX="866.0" layoutY="687.0" mnemonicParsing="false" onMouseClicked="#onBtnFileClicked" text="Upload / Download CSV Files" />
         </children>
      </VBox>
   </children>
</AnchorPane>
